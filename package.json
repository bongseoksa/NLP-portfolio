{
  "name": "nlp-portfolio",
  "version": "1.0.0",
  "type": "module",
  "description": "GitHub Repository Analyzer with NLP/RAG",
  "scripts": {
    "test:supabase": "tsx scripts/test-supabase.ts",
    "test:api": "tsx scripts/test-api.ts",
    "embed": "tsx scripts/unified-embedding-pipeline.ts",
    "embed:reset": "tsx scripts/unified-embedding-pipeline.ts --reset",
    "migrate:vector": "tsx scripts/migrate-vector-dimension.ts",
    "local_export": "tsx scripts/export-embeddings.ts",
    "server": "SKIP_ENV_VALIDATION=true vercel dev --listen 3002 --yes",
    "dev:frontend": "vite",
    "dev": "concurrently \"pnpm run server\" \"pnpm run dev:frontend\" --names \"API,FRONTEND\" --prefix-colors \"blue,green\"",
    "build:frontend": "panda codegen && tsc -b && vite build",
    "preview:frontend": "vite preview",
    "panda": "panda codegen",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"api/**/*.ts\" \"shared/**/*.ts\" \"scripts/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" \"api/**/*.ts\" \"shared/**/*.ts\" \"scripts/**/*.ts\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/generative-ai": "^0.24.1",
    "@octokit/rest": "^22.0.1",
    "@supabase/supabase-js": "^2.87.1",
    "@tanstack/react-query": "^5.64.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jotai": "^2.12.3",
    "modern-normalize": "^3.0.1",
    "openai": "^4.73.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.3",
    "recharts": "^3.6.0",
    "tiktoken": "^1.0.22"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.53.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vercel/node": "^5.1.8",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "prettier": "^3.4.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.21.0",
    "vercel": "^41.7.1",
    "vite": "^6.0.7"
  }
}
